        android:layout_marginStart="33dp"
        android:layout_marginTop="56dp"
        android:background="#C483CE"
        app:layout_constraintStart_toEndOf="@+id/t4c4"
        app:layout_constraintStart_toStartOf="@+id/t4c4"
        app:layout_constraintTop_toTopOf="@+id/t1c5"
        tools:viewBindingIgnore="true" />

    <TextView
