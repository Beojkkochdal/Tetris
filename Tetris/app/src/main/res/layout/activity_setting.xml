        app:layout_constraintStart_toEndOf="@+id/t4c2"
        app:layout_constraintStart_toStartOf="@+id/t4c2"
        app:layout_constraintTop_toTopOf="@+id/t1c3"
        tools:viewBindingIgnore="true" />

    <TextView
        android:id="@+id/t5c6"
