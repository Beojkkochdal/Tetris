        app:layout_constraintStart_toEndOf="@+id/t4c5"
        app:layout_constraintStart_toStartOf="@+id/t4c5"
        app:layout_constraintTop_toTopOf="@+id/t1c6"
        tools:viewBindingIgnore="true" />

    <TextView
        android:id="@+id/t4c2"
        android:layout_width="25dp"
        android:layout_height="40dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="56dp"
        android:background="#96B653"
        app:layout_constraintStart_toEndOf="@+id/t4c1"
        app:layout_constraintStart_toStartOf="@+id/t4c1"
        app:layout_constraintTop_toTopOf="@+id/t1c2"
        tools:viewBindingIgnore="true" />

    <TextView
        android:id="@+id/t5c4"
        android:layout_width="25dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:background="#F77F49"
        app:layout_constraintStart_toEndOf="@+id/t5c3"
        app:layout_constraintTop_toBottomOf="@+id/t4c4"
        tools:viewBindingIgnore="true" />

    <TextView
        android:id="@+id/t5c1"
        android:layout_width="25dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:background="#F6487C"
        app:layout_constraintStart_toEndOf="@+id/radioGroup"
        app:layout_constraintTop_toBottomOf="@+id/t4c1"
